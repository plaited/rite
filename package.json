{
  "name": "@plaited/rite",
  "version": "3.1.1",
  "license": "ISC",
  "engines": {
    "node": ">= v18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaited/rite.git"
  },
  "bugs": {
    "url": "https://github.com/plaited/rite/issues"
  },
  "homepage": "https://github.com/plaited/rite/tree/main#readme",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./framework": "./dist/framework.js"
  },
  "files": [
    "./dist/**"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "node scripts/dev.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs",
    "lint:fix": "yarn lint --fix --quiet",
    "prepare": "husky install",
    "test": "yarn test:assert && yarn test:helpers",
    "test:assert": "jest src/**/*.test.ts",
    "test:helpers": "web-test-runner"
  },
  "lint-staged": {
    "*.(js|cjs|tsx|ts)": [
      "yarn lint:fix"
    ],
    "package.json": [
      "format-package -w"
    ]
  },
  "browserslist": [
    "> 2%"
  ],
  "packageManager": "yarn@3.6.0",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@plaited/utils": "3.1.0",
    "@web/test-runner-core": "0.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@jest/globals": "29.5.0",
    "@playwright/test": "1.35.1",
    "@swc/core": "1.3.67",
    "@swc/jest": "0.2.26",
    "@types/sinon": "10.0.15",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@web/dev-server": "0.2.3",
    "@web/dev-server-esbuild": "0.4.1",
    "@web/test-runner": "0.16.1",
    "@web/test-runner-playwright": "0.10.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.43.0",
    "eslint-plugin-compat": "4.1.4",
    "format-package": "7.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "pinst": "3.0.0",
    "playwright": "1.35.1",
    "sinon": "15.2.0",
    "typescript": "5.1.6"
  }
}
